  "version": "0.2.0",
  "configurations": [
      {
          "type": "firefox",
          "request": "attach",
          "name": "Attach"
      },
    
    {
      "name": "Run Windows Hermes - Experimental",
      "request": "launch",
      "type": "reactnativedirect",
      "cwd": "${workspaceFolder}",
      "enableDebug": false,
      "platform": "windows"
    },
    {
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "name": "Launch index.html",
      "file": "${workspaceFolder}/index.html"
    }
  ]
}
